{
  "name": "react-visual-docs-cli",
  "version": "1.0.0",
  "description": "Command-line interface for generating json docs of React project to be consumed by react-visual-docs-ui",
  "main": "./lib/index.js",
  "scripts": {
    "build": "babel src -d lib",
    "start": "nodemon --exec babel-node --presets es2015,stage-0 -- src/index.js && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "react-visual-docs-cli": "./lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidleger95/react-visual-docs-cli.git"
  },
  "keywords": [
    "react",
    "node",
    "react-docgen",
    "cli",
    "javascript"
  ],
  "author": "David Leger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidleger95/react-visual-docs-cli/issues"
  },
  "homepage": "https://github.com/davidleger95/react-visual-docs-cli#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babylon": "^5.8.38",
    "cli-spinner": "^0.2.6",
    "commander": "^2.11.0",
    "fs-sync": "^1.0.4",
    "lodash": "^4.17.4",
    "nodemon": "^1.11.0",
    "react-docgen": "^2.17.0"
  }
}
